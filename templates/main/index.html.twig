                        <hr>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!--Resume Section-->
    <section class="section" id="resume">
        <div class="container">
            <h2 class="mb-5"><span class="text-danger">Mon</span> Résumé</h2>
            <div class="row">
                <div class="col-md-6 col-lg-4">
                    <div class="card">
                       <div class="card-header">
                            <div class="mt-2">
                                <h4>Scolaire</h4>
                                <span class="line"></span>  
                            </div>
                        </div>
                        <div class="card-body">
                            {% for studie in studies %}
                                <h6 class="title text-danger">{{ studie.years }}</h6>
                                <P>{{ studie.place }}</P>
                                <P class="subtitle">{{ studie.address }}<br>{{ studie.text }}</P>
                                {% if not loop.last %}
                                    <hr>
                                {% endif %}
                            {% endfor %}
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-lg-4">
                    <div class="card">
                       <div class="card-header">
                            <div class="mt-2">
                                <h4>Projets</h4>
                                <span class="line"></span>  
                            </div>
                        </div>
                        <div class="card-body">
						    {% for project in projects %}
								<h6 class="title text-danger">{{ project.year|date('Y') }}</h6>
                                <P>{{ project.name }}</P>
                                <P class="subtitle">{% if project.link !="" %}<a href="{{ project.link }}" target="_blank">Lien ici</a><br>{% endif %}{{ project.description }}</P>
                                {% if not loop.last %}
                                    <hr>
                                {% endif %}
                            {% endfor %}
                            
                        </div>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="card">
                       <div class="card-header">
                            <div class="pull-left">
                                <h4 class="mt-2">Compétences</h4>
                                <span class="line"></span>  
                            </div>
                        </div>
                        <div class="card-body pb-2">
                            {% for skill in skills %}
                                <h6>{{ skill.name }}</h6>
                                <div class="progress mb-3">
                                    <div class="progress-bar bg-danger" role="progressbar" style="width: {{skill.value}}%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                </div>
                            {% endfor %}
                        </div>
                    </div>
                    <div class="card">
                       <div class="card-header">
                            <div class="pull-left">
                                <h4 class="mt-2">Langages</h4>
                                <span class="line"></span>  
                            </div>
                        </div>
                        <div class="card-body pb-2">
                            {% for language in languages %}
                                <h6>{{ language.name }}</h6>
                                <div class="progress mb-3">
                                    <div class="progress-bar bg-danger" role="progressbar" style="width: {{ language.value }}%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                </div>
                            {% endfor %}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section class="section bg-dark text-center">
        <div class="container">
            <div class="row text-center">
                <div class="col-md-6 col-lg-3">
                    <div class="row ">
                        <div class="col-5 text-right text-light border-right py-3">
                            <div class="m-auto"><i class="ti-alarm-clock icon-xl"></i></div>
                        </div>
                        <div class="col-7 text-left py-3">
                            <h1 class="text-danger font-weight-bold font40">{{ hour.value }}</h1>
                            <p class="text-light mb-1">Heures passés sur les projets</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-lg-3">
                    <div class="row">
                        <div class="col-5 text-right text-light border-right py-3">
                            <div class="m-auto"><i class="ti-layers-alt icon-xl"></i></div>
                        </div>
                        <div class="col-7 text-left py-3">
                            <h1 class="text-danger font-weight-bold font40">{{ projects|length }}</h1>
                            <p class="text-light mb-1">Projets travaillés</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-lg-3">
                    <div class="row">
                        <div class="col-5 text-right text-light border-right py-3">
                            <div class="m-auto"><i class="ti-face-smile icon-xl"></i></div>
                        </div>
                        <div class="col-7 text-left py-3">
                            <h1 class="text-danger font-weight-bold font40">{{ happy.value }}</h1>
                            <p class="text-light mb-1">Retours positifs</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-lg-3">
                    <div class="row">
                        <div class="col-5 text-right text-light border-right py-3">
                            <div class="m-auto"><i class="ti-heart-broken icon-xl"></i></div>
                        </div>
                        <div class="col-7 text-left py-3">
                            <h1 class="text-danger font-weight-bold font40"> +∞</h1>
                            <p class="text-light mb-1">Cafés bu</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section class="section" id="service">
        <div class="container">
            <h2 class="mb-5 pb-4"><span class="text-danger">Ma</span> Formation</h2>
            <div class="row">
                <div class="col-md-4 col-sm-6">
                    <div class="card mb-5">
                       <div class="card-header has-icon">
                            <i class="ti-vector text-danger" aria-hidden="true"></i>
                        </div>
                        <div class="card-body px-4 py-3">
                            <h5 class="mb-3 card-title text-dark">BTS SIO</h5>
                            <P class="subtitle">Le Brevet de Technicien Supérieur en Services Informatiques aux Organisations (BTS SIO) est destiné à ceux qui cherchent à se spécialiser en deux ans dans les métiers de développeur ou d'administrateur réseau. Ce programme permet aux étudiants de se préparer directement à intégrer le marché du travail ou de poursuivre leurs études dans le domaine de l'informatique.</P>
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="card mb-5">
                       <div class="card-header has-icon">
                            <i class="ti-package text-danger" aria-hidden="true"></i>
                        </div>
                        <div class="card-body px-4 py-3">
                            <h5 class="mb-3 card-title text-dark">SLAM</h5>
                            <P class="subtitle">SLAM, pour Solutions Logicielles et Applications Métier, forme aux métiers de la conception et de la maintenance de programmes informatiques en entreprise. Les étudiants apprennent à gérer les parcs informatiques, administrer les réseaux, intégrer, sécuriser et configurer les serveurs ainsi que les postes clients et équipements d'interconnexion.</P>
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="card mb-5">
                       <div class="card-header has-icon">
                            <i class="ti-write text-danger" aria-hidden="true"></i>
                        </div>
                        <div class="card-body px-4 py-3">
                            <h5 class="mb-3 card-title text-dark">SISR</h5>
                            <P class="subtitle">L'option "Solutions d'Infrastructure, Systèmes et Réseaux" (SISR) est axée sur la formation des étudiants aux métiers impliquant la conception et la maintenance des infrastructures réseau. Les futurs administrateurs, techniciens ou pilotes d'exploitation seront responsables d'assurer la sécurité, la maintenance et l'installation des réseaux et des équipements informatiques.</P>
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="card mb-5">
                       <div class="card-header has-icon">
                            <i class="ti-map-alt text-danger" aria-hidden="true"></i>
                        </div>
                        <div class="card-body px-4 py-3">
                            <h5 class="mb-3 card-title text-dark">Poursuite d'études</h5>
                            <P class="subtitle">
                                <ul>
                                    <li>Licence informatique</li>
                                    <li>Licence informatique, mathématiques</li>
                                    <li>Licence pro métiers de l'informatique : applications web</li>
                                    <li>Licence pro métiers de l'informatique : conception, développement et test de logiciels</li>
                                </ul>
                            </P>
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="card mb-5">
                       <div class="card-header has-icon">
                            <i class="ti-bar-chart text-danger" aria-hidden="true"></i>
                        </div>
                        <div class="card-body px-4 py-3">
                            <h5 class="mb-3 card-title text-dark">Débouchés SLAM</h5>
                            <P class="subtitle">
                                <ul>
                                    <li>Programmeur d'applications</li>
                                    <li>Chargé d'études informatiques</li>
                                    <li>Responsable des services applicatifs</li>
                                    <li>Technicien d'études informatiques</li>
                                    <li>Informaticien d'études</li>
                                    <li>Développeur informatique</li>
                                    <li>Programmeur analyste</li>
                                    <li>Développeur d'applications informatiques</li>
                                </ul>

                            </P>
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="card mb-5">
                       <div class="card-header has-icon">
                            <i class="ti-support text-danger" aria-hidden="true"></i>
                        </div>
                        <div class="card-body px-4 py-3">
                            <h5 class="mb-3 card-title text-dark">Débouchés SISR</h5>
                            <P class="subtitle">
                                <ul>
                                    <li>Support systèmes et réseaux</li>
                                    <li>Administrateur systèmes et réseaux</li>
                                    <li>Technicien réseau et télécoms</li>
                                    <li>Informaticien support et déploiement</li>
                                    <li>Pilote d’exploitation</li>
                                    <li>Technicien d’infrastructure</li>
                                    <li>Technicien de production</li>
                                    <li>Technicien systèmes et réseaux</li>
                                </ul>
                            </P>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Portfolio Section -->
    <section class="section bg-custom-gray" id="portfolio">
        <div class="container">
            <h1 class="mb-5"><span class="text-danger">Mes</span> Documentations    </h1>
            <div class="portfolio">
                <div class="filters">
                    <a href="#" data-filter=".all" class="active">
                        Tout
                    </a>
                {% for documentationcategory in documentationcategorys %}
                    <a href="#" data-filter=".{{ documentationcategory.name }}">
                        {{ documentationcategory.name }}
                    </a>
                {% endfor %}
                </div>
                <div class="portfolio-container"> 
                    {% for documentation in documentations %}
                        <div class="col-md-6 col-lg-4 {{ documentation.getCategory().name }} new  all">
                            <div class="portfolio-item">
                                <img src="/uploads/images/{{ documentation.getCategory().img }}" class="img-fluid" alt="Download free bootstrap 4 admin dashboard, free boootstrap 4 templates">
                                <div class="content-holder">
                                    <a class="img-popup" href="/assets/imgs/web-1.jpg"></a>
                                    <div class="text-holder">
                                        <h6 class="title">{{ documentation.name }}</h6>
                                        <button class="btn btn-secondary" onClick="redirectToPage('{{ path('pdf_files', { filename: documentation.pdf }) }}')">Ouvrir</button>
                                    </div>
                                </div>   
                            </div>             
                        </div>
                    {% endfor %}
                </div> 
            </div>  
        </div>            
    </section>
